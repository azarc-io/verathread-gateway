{
  "name": "generate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "generate": "yarn schema && yarn types && yarn private && yarn public",
    "schema": "yarn graphql-codegen",
    "types": "go run ./generate.go -config=gqlgen_types.yml",
