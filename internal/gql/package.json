    "public": "go run ./generate.go -config=gqlgen_public.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "graphql": "^16.9.0"
  }
}
